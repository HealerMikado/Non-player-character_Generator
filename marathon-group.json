{
  "id": "GROUP_ID/",
  "apps": [
    {
      "id": "back",
      "cmd": null,
      "cpus": 1,
      "mem": 512,
      "disk": 0,
      "instances": 1,
      "constraints": [
        [
          "type",
          "IS",
          "boss"
        ]
      ],
      "container": {
        "type": "DOCKER",
        "docker": {
          "forcePullImage": true,
          "image": "docker-registry.beta.innovation.insee.eu/coding_dojo/pnj_generator_java",
          "parameters": [],
          "privileged": false
        },
        "volumes": [],
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 0,
            "labels": {},
            "protocol": "tcp",
            "servicePort": 10304
          }
        ]
      },
      "healthChecks": [
        {
          "gracePeriodSeconds": 300,
          "intervalSeconds": 60,
          "maxConsecutiveFailures": 3,
          "portIndex": 0,
          "protocol": "MESOS_TCP",
          "ipProtocol": "IPv4",
          "timeoutSeconds": 20,
          "delaySeconds": 15
        }
      ],
      "labels": {
        "HAPROXY_GROUP": "external",
        "HAPROXY_0_ENABLED": "true",
        "HAPROXY_0_VHOST": "pnj-generator.dev.innovation.insee.eu"
      },
      "networks": [
        {
          "mode": "container/bridge"
        }
      ],
      "portDefinitions": [],
      "maxLaunchDelaySeconds": 300
    }
  ]
}